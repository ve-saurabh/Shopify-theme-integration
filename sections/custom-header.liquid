    },
    {
      "type": "image_picker",
      "label": "Select Instagram Icon",
      "id": "social_icon_5"
    },
    {
      "type": "url",
      "label": "Instagram Link",
      "id": "instagram_url"
    }
    ],
    "blocks": [
      {
        "type": "usp_slider",
        "name": "Usp Item",
        "settings": [
          {
            "type": "image_picker",
            "id": "image",
            "label": "USP Image"
          }, {
            "type": "text",
            "id": "text",
            "label": "USP Text"
          }
        ]
      },
      {
        "type": "nav_block",
